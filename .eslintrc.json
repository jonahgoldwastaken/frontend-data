{
  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": 2020
  },
  "env": {
    "node": true,
    "es6": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:functional/external-recommended",
    "plugin:functional/recommended",
    "prettier"
  ],
  "plugins": ["functional", "prettier"],
  "rules": {
    "prettier/prettier": "error",
    "no-undef": "error"
  }
}
